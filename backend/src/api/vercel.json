{
  "version": 2,
  "builds": [
    {
      "src": "/backend/dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/profile/(.*)",
      "dest": "/backend/dist/index.js"
    },
    {
      "src": "/api/v1/auth/(.*)",
      "dest": "/backend/dist/index.js"
    },
    {
      "src": "/backend/(.*)",
      "dest": "/backend/dist/index.js"
    }
  ]
}
